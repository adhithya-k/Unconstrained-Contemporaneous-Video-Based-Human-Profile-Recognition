    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "def imgPyramid(img):\n",
    "    layer = img.copy()\n",
    "    gaussian_pyramid_list = [layer]\n",
    "\n",
    "    for i in range(4):\n",
    "        layer = cv2.pyrUp(layer)\n",
    "        gaussian_pyramid_list.append(layer)\n",
    "    \n",
    "    return gaussian_pyramid_list\n"
   ]
